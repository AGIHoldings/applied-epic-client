{
  "name": "@agiholdings/applied-epic-client",
  "version": "0.1.0",
  "description": "TypeScript client for Applied Epic API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --sourcemap",
    "dev": "tsup src/index.ts --dts --format esm --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "format": "biome format --write .",
    "generate": "tsx scripts/generate.ts",
    "release": "changeset version && pnpm i -r && changeset publish",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "api",
    "client",
    "typescript",
    "rest",
    "applied-epic"
  ],
  "author": "AGI Holdings",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/agiholdings/applied-epic-client.git"
  },
  "bugs": {
    "url": "https://github.com/agiholdings/applied-epic-client/issues"
  },
  "homepage": "https://github.com/agiholdings/applied-epic-client#readme",
  "peerDependencies": {},
  "dependencies": {
    "undici": "^6.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
